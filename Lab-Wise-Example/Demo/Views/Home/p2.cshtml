@{
    var Students = new[]{
        new {
            name = "Akash",
            details = new[]
            {
                    new{sem = 1 , spi = 9.1 }
            }
        },
        new {
            name = "Sky",
            details = new[]
            {
                    new{sem = 1 , spi = 5.1 }
            }
        }
    };
}

