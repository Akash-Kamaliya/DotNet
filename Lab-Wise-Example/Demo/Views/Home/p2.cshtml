<table class="table">
    <thead>
        <tr>
            
            <th scope="col">Name</th>
            <th scope="col">Semester</th>
            <th scope="col">Spi</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Students)
        {
            <tr>
                <td>@student.name</td>
                @foreach (var detail in student.details)
                {
                    <td>@detail.sem</td>
                    <td>@detail.spi</td>
                }
            </tr>
        }

    </tbody>
</table>
